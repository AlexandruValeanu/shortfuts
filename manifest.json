{
    "manifest_version": 2,
    "name": "shortfuts",
    "description": "Chrome extension to expose keyboard shortcuts for FIFA 18 FUT web app.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://www.easports.com/fifa/ultimate-team/web-app/*", "http://www.easports.com/fifa/ultimate-team/web-app/*"],
            "js": ["script.js"]
        }
    ]
}
